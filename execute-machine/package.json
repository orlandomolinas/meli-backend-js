{
  "name": "execute-machine",
  "version": "1.1.0",
  "main": "src/index.ts",
  "files": [
    "/src"
  ],
  "scripts": {
    "coverage": "nyc -r lcov -r text -e .ts -x \"test/*/.test.ts\" npm run test",
    "test": "jest --coverage",
    "prebuild": "rm -rf dist; cp package.json src/",
    "audit": "npm audit",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "prettier:list": "prettier --list-different \"./{test,src}/**/*\" || ECHO .",
    "prettier:write": "prettier --write  \"./{test,src}/**/*\"",
    "prettier": "prettier --check  \"./{test,src}/**/*\"",
    "sonar": "sonar-scanner -Dsonar.organization=$SONAR_ORGANIZATION -Dsonar.host.url=$SONAR_URL -Dsonar.login=$SONAR_TOKEN",
    "build": "npm run clean && npm run build-app",
    "clean": "rimraf dist && rimraf coverage && rimraf execute-machine.zip",
    "build-app": "tsc -p tsconfig.json",
    "postbuild": "npm run copy-pck-files && npm run copy-opentelemetry-files && npm run copy-mdl-files && npm run zip && mv execute-machine.zip dist/ && rm src/package.json",
    "copy-pck-files": "copyfiles -u 1 src/package.json dist/src",
    "copy-mdl-files": "cp package.json dist && cp .npmrc dist && cd dist && npm i --production && cd ../",
    "copy-opentelemetry-files": "cp config.yml dist/",
    "zip": "cd dist/ && bestzip  ../execute-machine.zip *"
  },
  "devDependencies": {
    "@types/jasmine": "^3.9.0",
    "@types/jest": "^27.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "axios-mock-adapter": "^1.18.1",
    "bestzip": "^2.1.7",
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "copyfiles": "^2.3.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "fs": "0.0.1-security",
    "jasmine": "^3.9.0",
    "jest": "^27.2.1",
    "jshint": "^2.10.2",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "repack-zip": "^0.2.5",
    "sonar-scanner": "^3.1.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "aws-sdk": "^2.1163.0",
    "aws-sdk-mock": "^5.7.0",
    "axios": "^0.27.2",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.34",
    "reflect-metadata": "^0.1.13"
  }
}
